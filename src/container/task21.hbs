             <p>{{text}}</p>

             {{#with button_text}}
             <button class="btn btn-dark">{{solid}}</button>
             <br>
             <button class="btn btn-outline-dark m-1">{{outline}}</button>
             {{/with}}
             </main>
            {{/with}}
           </div>
           </div>
        {{/with}}
  </div>
</div>
        
