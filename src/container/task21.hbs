           <p>{{text}}</p>

           {{#with button_text}}
           <button class="btn btn-secondary">{{solid}}</button>
           <br>
           <button class="btn btn-outline-secondary m-1">{{outline}}</button>
           {{/with}}
           </main>
           {{/with}}
           </div>
           </div>
        <br>
        <br>

